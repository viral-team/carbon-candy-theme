{
  "name": "carbon-candy",
  "icon": "img/app-logo.png",
  "displayName": "Carbon Candy",
  "description": "Carbon Candy Theme for VS Code",
  "version": "0.2.4",
  "publisher": "viral-team",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "viral team",
    "themes",
    "theme",
    "dark theme",
    "light theme",
    "carbon candy",
    "carbon",
    "candy",
    "anthracite",
    "dark",
    "graphene",
    "graphene pink",
    "graphene purple",
    "graphene gradient v1",
    "graphene gradient v2",
    "graphene gradient v3",
    "graphene gradient v4",
    "bee",
    "palenight",
    "obsidian",
    "aurora"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/viral-team/carbon-candy-theme"
  },
  "contributes": {
    "themes": [
      {
        "id": "carbon-candy-anthracite",
        "label": "Carbon Candy Anthracite",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-anthracite.json"
      },
      {
        "id": "carbon-candy-dark",
        "label": "Carbon Candy Dark",
        "uiTheme": "hc-black",
        "path": "./themes/carbon-candy-dark.json"
      },
      {
        "id": "carbon-candy-graphene",
        "label": "Carbon Candy Graphene",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene.json"
      },
      {
        "id": "carbon-candy-graphene-pink",
        "label": "Carbon Candy Graphene Pink",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-pink.json"
      },
      {
        "id": "carbon-candy-graphene-purple",
        "label": "Carbon Candy Graphene Purple",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-purple.json"
      },
      {
        "id": "carbon-candy-graphene-bee",
        "label": "Carbon Candy Graphene Bee",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-bee.json"
      },
      {
        "id": "carbon-candy-graphene-gradient-v1",
        "label": "Carbon Candy Graphene Gradient V1",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-gradient-v1.json"
      },
      {
        "id": "carbon-candy-graphene-gradient-v2",
        "label": "Carbon Candy Graphene Gradient V2",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-gradient-v2.json"
      },
      {
        "id": "carbon-candy-graphene-gradient-v3",
        "label": "Carbon Candy Graphene Gradient V3",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-gradient-v3.json"
      },
      {
        "id": "carbon-candy-graphene-gradient-v4",
        "label": "Carbon Candy Graphene Gradient V4",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-gradient-v4.json"
      },
      {
        "id": "carbon-candy-graphene-palenight",
        "label": "Carbon Candy Palenight",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-palenight.json"
      },
      {
        "id": "carbon-candy-carbon",
        "label": "Carbon Candy Carbon",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-carbon.json"
      },
      {
        "id": "carbon-candy-obsidian",
        "label": "Carbon Candy Obsidian",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-obsidian.json"
      },
      {
        "id": "carbon-candy-aurora",
        "label": "Carbon Candy Aurora",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-aurora.json"
      },
      {
        "id": "carbon-candy-aurora-thin-border",
        "label": "Carbon Candy Aurora Thin Border",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-aurora-thin-border.json"
      },
      {
        "id": "carbon-candy-carbon-thin-border",
        "label": "Carbon Candy Carbon Thin Border",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-carbon-thin-border.json"
      },
      {
        "id": "carbon-candy-dark-thin-border",
        "label": "Carbon Candy Dark Thin Border",
        "uiTheme": "hc-black",
        "path": "./themes/carbon-candy-dark-thin-border.json"
      },
      {
        "id": "carbon-candy-graphene-thin-border",
        "label": "Carbon Candy Graphene Thin Border",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-thin-border.json"
      }
      ,
      {
        "id": "carbon-candy-graphene-pink-thin-border",
        "label": "Carbon Candy Graphene Pink Thin Border",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-pink-thin-border.json"
      },
      {
        "id": "carbon-candy-graphene-purple-thin-border",
        "label": "Carbon Candy Graphene Purple Thin Border",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon-candy-graphene-purple-thin-border.json"
      }
    ],
    "iconThemes": [
      {
        "id": "carbon-candy-file-icons",
        "label": "Carbon Candy File Icons",
        "path": "./icons/carbon-candy-file-icon-theme.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "carbon-candy-product-icons",
        "label": "Carbon Candy Product Icons",
        "path": "./productIcons/carbon-candy-product-icon-theme.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Carbon Candy",
      "properties": {
        "carbonCandy.hidesExplorerArrows": {
          "type": "boolean",
          "default": false,
          "description": "Hide arrow icons in the explorer section."
        },
        "carbonCandy.folders.associations": {
          "type": "object",
          "default": {},
          "description": "With this configuration can customize the folder icons."
        },
        "carbonCandy.files.associations": {
          "type": "object",
          "default": {},
          "description": "With this configuration can customize the files icons."
        }
      }
    }
  }
}